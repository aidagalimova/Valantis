{
  "compilerOptions": {
    "target": "es2015",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "react-jsx",
    "baseUrl": "./src/"
  },
  "include": ["./src/**/*.ts", "./src/**/*.tsx"]
}
